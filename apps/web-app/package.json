{
  "name": "@strixy/web-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "open-next build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "yarn get:env drizzle-kit migrate",
    "db:push": "yarn get:env drizzle-kit push",
    "db:push:dev": "ENV=dev yarn db:push",
    "db:push:prod": "ENV=prod yarn db:push",
    "db:studio": "drizzle-kit studio",
    "deploy": "sst deploy --stage $ENV",
    "next:dev": "sst shell --stage dev -- next dev",
    "email:dev": "email dev --dir ./src/server/email/emails --port 3001",
    "dev": "run-p next:dev db:studio email:dev",
    "format": "prettier --ignore-path '../../.prettierignore' --write .",
    "format:check": "prettier --ignore-path '../../.prettierignore' --check .",
    "get:env": "env-cmd -f .env${ENV:+.$ENV}",
    "lint": "NODE_ENV=production next lint --fix",
    "lint:check": "NODE_ENV=production next lint",
    "next:build": "cp .env.$ENV .env.production && next build || next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "validate": "run-s typecheck lint format",
    "validate:check": "run-s typecheck lint:check format:check"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "1.4.1",
    "@aws-sdk/client-sesv2": "^3.637.0",
    "@aws-sdk/s3-request-presigner": "^3.627.0",
    "@emotion/cache": "^11.13.1",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@hookform/resolvers": "^3.9.0",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "@mui/material-nextjs": "^5.16.6",
    "@mui/x-date-pickers": "^7.14.0",
    "@react-email/components": "^0.0.23",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.50.0",
    "@trpc/client": "^11.0.0-rc.446",
    "@trpc/react-query": "^11.0.0-rc.446",
    "@trpc/server": "^11.0.0-rc.446",
    "axios": "^1.7.4",
    "date-fns": "^2.30.0",
    "drizzle-orm": "^0.30.10",
    "geist": "^1.3.0",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "material-react-table": "^2.13.1",
    "next": "^14.2.4",
    "next-auth": "^4.24.7",
    "next-safe-action": "^7.5.0",
    "open-next": "^3.0.8",
    "papaparse": "^5.4.1",
    "postgres": "^3.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.52.2",
    "react-hook-form-mui": "^7.0.1",
    "server-only": "^0.0.1",
    "sst": "ion",
    "superjson": "^2.2.1",
    "tss-react": "^4.9.12",
    "use-immer": "^0.10.0",
    "yup": "^1.4.0",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.10",
    "@types/lodash": "^4",
    "@types/node": "^20.14.10",
    "@types/papaparse": "^5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "drizzle-kit": "^0.21.4",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.4",
    "eslint-plugin-drizzle": "^0.2.3",
    "npm-run-all": "^4.1.5",
    "react-email": "^3.0.1",
    "typescript": "^5.5.3"
  },
  "ct3aMetadata": {
    "initVersion": "7.36.2"
  },
  "packageManager": "yarn@3.6.3"
}
