{
  "name": "@strixy/common",
  "version": "1.0.0",
  "description": "Lambda function generated by Amplify",
  "main": "dist/index.js",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "run-p watch:tsc watch:types:alias",
    "build": "run-s clean tsc types:alias",
    "tsc": "tsc",
    "watch:tsc": "tsc -w",
    "types:alias": "tsc-alias",
    "watch:types:alias": "tsc-alias -w",
    "clean": "rm -rf ./dist && rm -rf ./build",
    "typecheck": "tsc --noEmit",
    "lint:check": "NODE_ENV=production eslint \"{src,test}/**/*.ts\" --report-unused-disable-directives --max-warnings 0",
    "lint": "yarn lint:check --fix",
    "format:check": "prettier --ignore-path '../../.prettierignore' --check .",
    "format": "prettier --ignore-path '../../.prettierignore' --write .",
    "validate:check": "run-s typecheck lint:check format:check",
    "validate": "run-s typecheck lint format"
  },
  "dependencies": {
    "aws-sdk": "2.1670.0",
    "json-beautify": "^1.1.1"
  },
  "devDependencies": {
    "@strixy/eslint-config": "*",
    "@strixy/tsconfig": "*",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.1.0",
    "eslint": "^8.50.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths-jest": "^0.0.1",
    "typescript": "^5.5.4"
  }
}
